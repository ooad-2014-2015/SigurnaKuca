    <Grid Height="443" Margin="0,4,-216,4">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="212"/>
        </Grid.ColumnDefinitions>
        <GroupBox Margin="0,0,19,37" HorizontalAlignment="Right" VerticalAlignment="Bottom" Width="475" Height="391" >
            <GroupBox.Header>
