                        <Paragraph>
                            <Run/>
                        </Paragraph>
                    </FlowDocument>
                </RichTextBox>
                <Label x:Name="label4" Height="23" Width="145" FontSize="11" FontFamily="Microsoft Sans Serif" TabIndex="1" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="-55" Canvas.Top="8" Content="Podaci o napretku terapije" />
                <RichTextBox x:Name="richTextBox_historijaRjesenjaPravnik" Height="337" Width="140" TabIndex="0" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="-55" Canvas.Top="31" >
                    <FlowDocument>
                        <Paragraph>
                            <Run/>
                        </Paragraph>
                    </FlowDocument>
                </RichTextBox>
                <Button x:Name="button_azurirajRjesenjePravnik" Content="Ažuriraj nalaze" Height="21" Canvas.Left="316" Canvas.Top="124" Width="77" RenderTransformOrigin="0.484,0.476"/>
