        <Button x:Name="button_pregledRasporedaPravnik" Content="Raspored" HorizontalAlignment="Left" Height="24" Margin="12,382,0,0" VerticalAlignment="Top" Width="84"/>
        <ListBox x:Name="listBox_listaPacijenataPravnik" HorizontalAlignment="Left" Height="345" Margin="141,61,0,0" VerticalAlignment="Top" Width="118"/>
