                            <Label x:Name="label13" Height="25" Width="72" FontSize="11" FontFamily="Microsoft Sans Serif" TabIndex="10" Margin="46,206,727,175" HorizontalAlignment="Right" VerticalAlignment="Bottom" />
                            <Label Name="label8" Height="25" Width="111" FontSize="11" FontFamily="Microsoft Sans Serif" TabIndex="9" Margin="11,84,723,297" HorizontalAlignment="Right" VerticalAlignment="Bottom" >Datum ro?enja:</Label>
                            <TextBox Name="textBox_passwordKorisnika" Height="23" Width="115" TabIndex="8" Margin="108,142,622,241" HorizontalAlignment="Right" VerticalAlignment="Bottom" TextWrapping="Wrap" ></TextBox>
                            <TextBox Name="textBox_usernameKorisnika" Height="23" Width="115" TabIndex="7" Margin="108,111,622,272" HorizontalAlignment="Right" VerticalAlignment="Bottom" TextWrapping="Wrap" ></TextBox>
                            <TextBox Name="textBox_prezimeKorisnika" Height="23" Width="115" TabIndex="6" Margin="108,48,622,335" HorizontalAlignment="Right" VerticalAlignment="Bottom" TextWrapping="Wrap" ></TextBox>
                            <TextBox Name="textBox_imeKorisnika" Height="23" Width="115" TabIndex="5" Margin="108,14,622,369" HorizontalAlignment="Right" VerticalAlignment="Bottom" TextWrapping="Wrap" ></TextBox>
                            <DatePicker Name="dateTimePicker_datRodjenjaKorisnika" Height="23" Width="115" TabIndex="4" Margin="108,79,622,304" HorizontalAlignment="Right" VerticalAlignment="Bottom" ></DatePicker>
