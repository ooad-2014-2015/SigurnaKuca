            <TabItem Name="tabPage3" Header="Upravljanje zahtjevima" Background="GhostWhite" >
                <Canvas Name="cantabPage3" Height="349" Width="655" Background="GhostWhite" >
                </Canvas>
            </TabItem>
            <TabItem Name="tabPage4" Header="Upravljanje rasporedima" Background="PapayaWhip" >
                <Canvas Name="cantabPage4" Height="349" Width="655" Background="PapayaWhip" >
                </Canvas>
            </TabItem>
        </TabControl>
