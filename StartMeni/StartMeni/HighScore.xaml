<Page
    x:Class="StartMeni.HighScore"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:StartMeni"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{StaticResource ApplicationPageBackgroundThemeBrush}" RenderTransformOrigin="0.51,0.551">
    	<Image x:Name="image_HighScore" HorizontalAlignment="Left" Height="96" Margin="285,39,0,0" VerticalAlignment="Top" Width="960" Source="Assets/highScore.png"/>
    	<ListView x:Name="listView_HighScore" HorizontalAlignment="Left" Height="529" Margin="285,185,0,0" VerticalAlignment="Top" Width="960" Background="#FF383838"/>
    	<Button x:Name="button_backHighScore" Content="Back" HorizontalAlignment="Left" Height="66" Margin="42,39,0,0" VerticalAlignment="Top" Width="138" Click="button_backHighScore_Click"/>
    	<Image x:Name="image_PlayerName" HorizontalAlignment="Left" Height="63" Margin="369,210,0,0" VerticalAlignment="Top" Width="348"/>
    	<Image x:Name="image_HighScore1" HorizontalAlignment="Left" Height="63" Margin="807,210,0,0" VerticalAlignment="Top" Width="348"/>

    </Grid>
</Page>